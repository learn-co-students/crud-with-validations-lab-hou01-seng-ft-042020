<% if flash[:errors]%>
    <% flash[:errors].each do |e| %>
        <li >
            <%= e %>
        </li>
    <%end%>
<%end%>

<%= form_for(@song) do |f|%>
<%= f.label :title %>
<%= f.text_field :title%>
<br>
<%= f.label :released %><br>
<%= f.check_box :released %>
<br>
<%= f.label :release_year %><br>
<%= f.number_field :release_year %>
<br>
<%= f.label :genre %><br>
<%= f.text_field :genre %>
<br>
<%= f.label :artist_name %><br>
<%= f.text_field :artist_name %>
<br>
<%= f.submit %>
<% end %>

